        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.junsir.community.community.Dao.UserDao">

    <select id="findUserById" parameterType="String" resultType="com.junsir.community.community.model.GitHubUser">
